    "babel-preset-react": "^6.23.0",
    "chalk": "^1.1.3",
    "electron-devtools-installer": "^2.1.0",
    "electron-prebuilt-compile": "^1.6.2",
    "eslint": "^3.18.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "^3.0.1",
    "npm-run-all": "^4.0.2"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
