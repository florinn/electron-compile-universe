{
  "name": "electron-react-seed",
  "version": "1.0.0",
  "description": "A basic electron react start kit",
  "main": "src/app.js",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
